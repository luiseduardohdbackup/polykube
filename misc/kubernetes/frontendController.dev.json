{
    "id": "frontendController",
    "kind": "ReplicationController",
    "apiVersion": "v1beta1",
    "desiredState": {
        "replicas": 1,
        "replicaSelector": {"name": "frontend"},
        "podTemplate": {
            "desiredState": {
                "manifest": {
                    "version": "v1beta1",
                    "id": "frontendController",
                    "containers": [
                        {
                            "name": "agora-api-name",
                            "image": "localhost:5000/agora-api",
                            "ports": [{ "containerPort": 5000, "hostPort": 5000 }]
                        }
                    ]
                }
            },
            "labels": {"name": "frontend"}
        }},
    "labels": {"name": "frontend"}
}
